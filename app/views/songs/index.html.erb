<% @songs.each do |song| %>
<table>
    <thead>
        <tr>
            <th><%= link_to  "Title", song_path(song) %></th>
            <th><%= "Release Year" if song.released? %></th>
            <th>Artist</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.release_year if song.released? %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
        </tr>
    </tbody>
</table>
<% end %>

<br>

<%= link_to  "New Song", new_song_path %>